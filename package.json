{
  "name": "hydro-js-integrations",
  "version": "0.0.12",
  "description": "Integrations for Vite and Astro and more TBC",
  "type": "module",
  "exports": {
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "default": "./dist/vite.js"
    },
    "./astro": {
      "types": "./dist/astro/index.d.ts",
      "import": "./dist/astro/index.js",
      "default": "./dist/astro/index.js"
    },
    "./astro/client.js": {
      "types": "./dist/astro/client.d.ts",
      "import": "./dist/astro/client.js",
      "default": "./dist/astro/client.js"
    },
    "./astro/server.js": {
      "types": "./dist/astro/server.d.ts",
      "import": "./dist/astro/server.js",
      "default": "./dist/astro/server.js"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Krutsch/hydro-js-integrations.git"
  },
  "keywords": [
    "ssr",
    "jsx",
    "vite",
    "astro-integration",
    "astro-component",
    "renderer",
    "hydro-js"
  ],
  "author": "Fabian Klingenberg <klingenberg.fabian@gmx.de> (https://klingenberg.works/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Krutsch/hydro-js-integrations/issues"
  },
  "homepage": "https://github.com/Krutsch/hydro-js-integrations#readme",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.21",
    "astro": "^5.7.14",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "happy-dom": "^17.4.7",
    "hydro-js": "^1.8.5",
    "jsdom": "^26.1.0"
  }
}
